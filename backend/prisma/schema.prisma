generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String
  name String
  dob DateTime
  age Int
}

model Jobs {
  id Int @id @default(autoincrement())
  jobid Int
  jobPoster String
  title String
  date_time DateTime
  ApplyUrl String

}